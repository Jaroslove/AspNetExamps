@model IEnumerable<AspNetExamps.Models.EF.Product>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table>
        <tr>
            <th>
                @Html.DisplayNameFor(a => a.Name)
            </th>
            <th>
                @Html.DisplayNameFor(a => a.Price)
            </th>
        </tr>
        @foreach (var item in Model)
        {
            <td>
                @Html.DisplayFor(a => item.Name)
            </td>
            <td>
                @Html.DisplayFor(a => item.Price)
            </td>
            @*<td>

            </td>*@
        }
    </table>
</body>
</html>
